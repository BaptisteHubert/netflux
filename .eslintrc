{
  "extends": [
    "conaclos/esnext-base",
    "standard"
  ],
  "rules": {
    "guard-for-in": 0,
    "no-underscore-dangle": 0,
    "default-case": 0,
    "no-console": 0,
    "no-continue": 0,
    "no-loop-func": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-sync": 0,
    "no-use-before-define": 0,
    "prefer-const": 0,
    "no-catch-shadow": 0,
    "no-bitwise": 0
  },
  "env": {
    "browser": true,
    "jasmine": true,
    "es6": true
  }
}
